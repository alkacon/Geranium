<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:input="urn:import:com.alkacon.geranium.client.ui.input">
	<ui:with field="images" type="com.alkacon.geranium.client.ui.css.I_ImageBundle" />
	<ui:with field="style" type="com.alkacon.geranium.client.ui.css.I_LayoutBundle" />
	<g:FlowPanel ui:field="m_wrapper">
		<g:FlowPanel ui:field="m_fieldset">
			<g:Image ui:field="m_image" resource="{images.arrowBottomImage}" styleName="{style.fieldsetCss.image}"></g:Image>
			<input:Label ui:field="m_legend" styleName="{style.fieldsetCss.legend}"></input:Label>
			<g:FlowPanel ui:field="m_content" styleName="{style.fieldsetCss.content}"></g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 